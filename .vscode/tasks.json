{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "npm: start - frontend",
      "type": "shell",
      "command": "npx cross-env BROWSER=none npm start",
      "options": {
        "cwd": "${workspaceFolder}/frontend"
      },
      "isBackground": true,
      "problemMatcher": [
        {
          "pattern": {
            "regexp": ".*",
            "file": 1,
            "location": 2,
            "message": 3
          },
          "background": {
            "activeOnStart": true,
            "beginsPattern": ".*Starting the development server.*",
            "endsPattern": ".*Compiled successfully.*"
          }
        }
      ]
    }
  ]
}
